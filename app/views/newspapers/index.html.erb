<h1>Listing newspapers</h1>
<%= paginate @newspapers %>

<% @newspapers.each do |newspaper| %>
  <table class="table table-stripped">
  <tr>
    <th>Title</th><td><%=  newspaper.title %></td>
  </tr>
  <tr>
    <th>Date</th><td><%= newspaper.pubDate %></td>
  </tr>
  <tr>
    <th>link</th><td><%= link_to '기사 읽기...', newspaper.link %></td>
  </tr>
  <tr>
    <th>Description</th><td><%= newspaper.description %></td>
  </tr>
  </table>
  <% if user_signed_in? %>
   <%= button_to '스크랩', clips_path(newspaper: newspaper), class: 'btn btn-mini' %>
  <% end %>

<% end %>


<br />
